<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main">
   <children>
      <TextField fx:id="TextToAdd" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="521.0" promptText="Text to enter " />
      <Button fx:id="Confirm" layoutX="535.0" layoutY="14.0" mnemonicParsing="false" onAction="#addToList" prefHeight="25.0" prefWidth="54.0" text="add" />
      <ListView fx:id="List" layoutX="14.0" layoutY="39.0" prefHeight="348.0" prefWidth="426.0" />
      <Button fx:id="SendAll" layoutX="440.0" layoutY="39.0" mnemonicParsing="false" onAction="#sendAll" prefHeight="25.0" prefWidth="148.0" text="Send" />
   </children>
</AnchorPane>
